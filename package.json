{
  "name": "bn-api-node",
  "version": "0.2.0",
  "description": "Bigneon Javascript Middleware",
  "main": "./dist/bundle.client.js",
  "scripts": {
    "test": "npm run build-tsc && ./node_modules/.bin/mocha test/index.js",
    "test-no-build": "./node_modules/.bin/mocha test/index.js",
    "build": "npm run build-tsc && ./node_modules/.bin/webpack --config ./build/webpack.config.js",
    "build-web": "./node_modules/.bin/webpack --config ./build/webpack.browser.js",
    "build-server": "./node_modules/.bin/webpack --config ./build/webpack.server.js",
    "build-tsc": "./node_modules/.bin/tsc",
    "db-seed": "npm run build-tsc && cd db-seed && node db-seed",
    "prepublish": "npm run build",
    "eslint-validate": "node_modules/eslint/bin/eslint.js  -c .eslintrc --ext .ts --ext .js ./src/",
    "eslint-fix": "node_modules/eslint/bin/eslint.js --fix -c .eslintrc --ext .ts --ext .js ./src/**/*",
    "prepare": "[ \"$NODE_ENV\" != \"production\" ] && git config core.hooksPath .githooks",
    "docs": "node_modules/.bin/typedoc --out docs && touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/big-neon/bn-api-node.git"
  },
  "keywords": [
    "bigneon"
  ],
  "author": "Bigneon",
  "license": "BSD 3-Clause",
  "bugs": {
    "url": "https://github.com/big-neon/bn-api-node/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/big-neon/bn-api-node#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@types/node": "^10.11.2",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "mocha": "^5.2.0",
    "ts-loader": "^4.5.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0",
    "webpack": "^4.17.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
