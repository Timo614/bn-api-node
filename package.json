{
  "name": "bn-api-node",
  "version": "2.0.7",
  "description": "Bigneon Javascript Middleware",
  "main": "./dist/bundle.client.js",
  "scripts": {
    "test": "npm run build:tsc && ./node_modules/.bin/mocha test/**/*.js",
    "test:no-build": "mocha test/index.js",
    "build": "tsc && tsc -m es6 --outDir dist-esm && webpack --config build/webpack.browser.js",
    "build:web": "webpack --config ./build/webpack.browser.js",
    "build:server": "webpack --config ./build/webpack.server.js",
    "build:tsc": "tsc",
    "build:docs": "typedoc --out docs && touch docs/.nojekyll",
    "build:changelog": "changelog -p",
    "db-seed": "npm run build:tsc && cd db-seed && node db-seed",
    "eslint:validate": "node_modules/eslint/bin/eslint.js  -c .eslintrc --ext .ts --ext .js ./src/",
    "eslint:fix": "node_modules/eslint/bin/eslint.js --fix -c .eslintrc --ext .ts --ext .js ./src/**/*",
    "prepublish": "npm run build",
    "prepare": "[ \"$NODE_ENV\" != \"production\" ] && git config core.hooksPath .githooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/big-neon/bn-api-node.git"
  },
  "keywords": [
    "bigneon"
  ],
  "author": "Bigneon",
  "license": "BSD 3-Clause",
  "bugs": {
    "url": "https://github.com/big-neon/bn-api-node/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/big-neon/bn-api-node#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@types/node": "^10.11.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "generate-changelog": "^1.7.1",
    "mocha": "^5.2.0",
    "stripe": "^6.20.0",
    "ts-loader": "^4.5.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0",
    "webpack": "^4.17.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
